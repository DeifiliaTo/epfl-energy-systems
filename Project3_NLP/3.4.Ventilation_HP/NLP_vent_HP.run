/*---------------------------------------------------------------------------------------------------------------------------------------
Load the main model and its data
---------------------------------------------------------------------------------------------------------------------------------------*/
reset;
model NLP_vent_HP.mod;
data  NLP_vent_HP.dat;
include Buildings_data/b1.dat;
include Buildings_data/b2.dat;


/*---------------------------------------------------------------------------------------------------------------------------------------
AMPL options
---------------------------------------------------------------------------------------------------------------------------------------*/
/*option solver './snopt';*/
option solver snopt;
#option omit_zero_rows 1;
#option omit_zero_cols 1;
option presolve_eps 8.53e-15;

/*---------------------------------------------------------------------------------------------------------------------------------------
Solve the problem
---------------------------------------------------------------------------------------------------------------------------------------*/
solve;

/*---------------------------------------------------------------------------------------------------------------------------------------
Display commands
---------------------------------------------------------------------------------------------------------------------------------------*/
display TC;
display OPEX;
display CAPEX;

display Area_Vent;
display Qheating;
display Heat_Vent;
display Trelease;


display E;
display E_2;

display Text;
display Text_new;
display Tair_in;
display Trelease;
display Trelease_2;

display Qevap_2;
display Qcond_2;
display COP_2;
display TLMCond_2;
display TLMEvapHP_2;
display Cost_HP;

