/*---------------------------------------------------------------------------------------------------------------------------------------
Load the main model and its data
---------------------------------------------------------------------------------------------------------------------------------------*/
reset;
model NLP_DC.mod;
data NLP_DC.dat;

/*---------------------------------------------------------------------------------------------------------------------------------------
AMPL options
---------------------------------------------------------------------------------------------------------------------------------------*/
/*option solver './snopt';*/
option solver snopt;
#option omit_zero_rows 1;
#option omit_zero_cols 1;

/*---------------------------------------------------------------------------------------------------------------------------------------
Solve the problem
---------------------------------------------------------------------------------------------------------------------------------------*/
solve;

/*---------------------------------------------------------------------------------------------------------------------------------------
Display commands
---------------------------------------------------------------------------------------------------------------------------------------*/
#These variables might be interest to print. Feel free to choose new ones!
display TC;

display TRadin;
display TDCout;
display AHEDC;
display dTLMDC;

display Qrad;
display E;
display TLMCond;
display Qevap;
display Qcond;
display COP;
display MassEPFL;
display MassDC;
display Qfree;
display THPin;
display Flow;

display OPEX;
display CAPEX;


