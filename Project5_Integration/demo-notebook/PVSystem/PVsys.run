/*---------------------------------------------------------------------------------------------------------------------------------------
Load the main model and its data
---------------------------------------------------------------------------------------------------------------------------------------*/
reset;
model PVsys.mod;
data PVsys.dat;




/*---------------------------------------------------------------------------------------------------------------------------------------
AMPL options
---------------------------------------------------------------------------------------------------------------------------------------*/
option solver 'C:\Program Files\ampl_mswin64\gurobi';                                         # choose the solver
option omit_zero_rows 1;										# to not display 0 in rows
option omit_zero_cols 1;										# to not display 0 in columns
option presolve 5e-05;
/*---------------------------------------------------------------------------------------------------------------------------------------
Solve the problem
---------------------------------------------------------------------------------------------------------------------------------------*/

solve;

/*---------------------------------------------------------------------------------------------------------------------------------------
Display commands
---------------------------------------------------------------------------------------------------------------------------------------*/
#display p_load,p_imp, p_exp, p_cha,p_dis,p_pv,p_cur,E_bat;
display b_pv,P_pv_cap, b_bat,E_bat_cap;

display obj_fcn['ox_ge'],obj_fcn['ox_pwr'],obj_fcn['opex'],obj_fcn['capex'],obj_fcn['totex'];


